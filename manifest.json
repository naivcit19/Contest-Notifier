{
  "manifest_version": 2,

  "name": "Contest Notifier",
  "description": "This extension notifies of upcoming coding events",
  "version": "1.0",
  "icons":{
		"128":"icon.png"
	},

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
 
  "permissions": ["tabs","http://*/*", "https://*/*"]
}